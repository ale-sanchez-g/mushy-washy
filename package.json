{
  "name": "mushgame",
  "version": "1.0.0",
  "description": "game using nodejs to 1 player to grown as big as  possiblie by eating only blue mushrooms.",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "test": "jest --coverage"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "reporters": [
      "default",
      [
        "jest-junit",
        {
          "outputDirectory": "./test-results/junit"
        }
      ]
    ],
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 1,
        "statements": 1
      }
    }
  },
  "author": "Sanchez Kerr Video Games",
  "license": "MIT",
  "dependencies": {
    "@launchdarkly/node-server-sdk": "^9.4.2",
    "express": "^4.19.2",
    "phaser": "^3.80.1"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-junit": "^16.0.0"
  }
}
